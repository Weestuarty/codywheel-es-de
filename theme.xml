<theme>
	<include>./navigationsounds.xml</include>
	<include>./colors.xml</include>
	<include>./systems/metadata-global/_default.xml</include>
	<include>./systems/metadata-global/${system.theme}.xml</include>
	<include>./systems/metadata-custom/${system.theme}.xml</include>

	<variables>
		<mainFont>./assets/fonts/acre.otf</mainFont>
		<secondaryFont>./assets/fonts/erbaum.otf</secondaryFont>
		<fontSize>0.0275</fontSize>
	</variables>

	<aspectRatio name="4:3">
		<include>./4-3.xml</include>
	</aspectRatio>

	<aspectRatio name="16:10">
		<include>./16-10.xml</include>
	</aspectRatio>

	<aspectRatio name="21:9">
		<include>./21-9.xml</include>
	</aspectRatio>


	<view name="system, gamelist">
		<image name="background">
			<pos>0 0</pos>
			<size>1 1</size>
			<origin>0 0</origin>
			<path>./assets/box.png</path>
			<tile>false</tile>
			<color>${backgroundColor}</color>
			<zIndex>0</zIndex>
		</image>
		<helpsystem name="help">
			<pos>.01 0.025</pos>
			<origin>0 0.5</origin>
			<textColor>${helpColor}</textColor>
			<textColorDimmed>${helpColor1}</textColorDimmed>
			<iconColor>${helpColor}</iconColor>
			<iconColorDimmed>${helpColor1}</iconColorDimmed>
			<fontPath>${mainFont}</fontPath>
			<fontSize>0.0162</fontSize>
			<opacity>0.6</opacity>
			<letterCase>capitalize</letterCase>
		</helpsystem>
	</view>

	<view name="system">
