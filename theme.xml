			<text>${system.fullName}</text>
			<textColor>${outlineColor1}</textColor>
			<fontPath>${mainFont}</fontPath>
			<fontSize>0.02</fontSize>
			<letterCase>none</letterCase>
			<lineSpacing>1</lineSpacing>
			<zIndex>50</zIndex>
		</carousel>
		<gameselector name="select">
